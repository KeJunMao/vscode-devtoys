{
  "name": "devtoys",
  "displayName": "DevToys",
  "icon": "media/icon.png",
  "description": "DevToys for VSCode",
  "version": "0.0.0-development",
  "publisher": "kejun",
  "license": "GPLv3",
  "engines": {
    "vscode": "^1.64.0"
  },
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/KeJunMao/vscode-devtoys"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:devtoys.showTool",
    "onView:github-kejun-devtoys"
  ],
  "main": "./dist/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "devtoys.showTool",
        "title": "%command.devtoys.showTool.title%",
        "category": "DevToys"
      },
      {
        "command": "devtoys.searchTool",
        "title": "%command.devtoys.searchTool.title%",
        "category": "DevToys",
        "icon": "$(search)"
      },
      {
        "command": "devtoys.automatically",
        "title": "%command.devtoys.automatically.title%",
        "category": "DevToys",
        "icon": "$(wand)"
      }
    ],
    "viewsContainers": {
      "activitybar": [
        {
          "id": "github-kejun-devtoys",
          "title": "DevToys",
          "icon": "media/tools.svg"
        }
      ]
    },
    "views": {
      "github-kejun-devtoys": [
        {
          "id": "github-kejun-devtoys",
          "name": "DevToys"
        }
      ]
    },
    "menus": {
      "view/title": [
        {
          "command": "devtoys.searchTool",
          "when": "view == github-kejun-devtoys",
          "group": "navigation@0"
        },
        {
          "command": "devtoys.automatically",
          "when": "view == github-kejun-devtoys",
          "group": "navigation@1"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "yarn run package",
    "compile": "yarn run i18n:build && rollup -c && webpack",
    "watch": "concurrently \"yarn run i18n:dev\" \"rollup -c -w\" \"webpack --watch\"",
    "package": "yarn run i18n:build && rollup -c && webpack --mode production --devtool hidden-source-map",
    "compile-tests": "tsc -p . --outDir test-out",
    "watch-tests": "tsc -p . -w --outDir test-out",
    "pretest": "yarn run compile-tests && yarn run compile && yarn run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./test-out/test/runTest.js",
    "i18n:build": "esno scripts/build-i18n.ts",
    "i18n:dev": "nodemon -w ./locales -e json --exec \"yarn run i18n:build\"",
    "new": "plop",
    "prepare": "husky install",
    "release": "semantic-release"
  },
  "devDependencies": {
    "@bjornlu/colorblind": "^1.0.3",
    "@commitlint/cli": "^16.2.1",
    "@commitlint/config-conventional": "^16.2.1",
    "@rollup/plugin-commonjs": "^21.0.1",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^13.1.3",
    "@rollup/plugin-replace": "^3.1.0",
    "@semantic-release/commit-analyzer": "^9.0.2",
    "@semantic-release/github": "^8.0.2",
    "@semantic-release/release-notes-generator": "^10.0.3",
    "@tsconfig/svelte": "^1.0.0",
    "@types/autosize": "^4.0.1",
    "@types/crypto-js": "^4.1.1",
    "@types/glob": "^7.2.0",
    "@types/he": "^1.1.2",
    "@types/mocha": "^9.0.0",
    "@types/node-forge": "^1.0.0",
    "@types/qrcode": "^1.4.2",
    "@types/react": "^17.0.39",
    "@types/react-dom": "^17.0.11",
    "@types/ua-parser-js": "^0.7.36",
    "@types/vscode": "^1.64.0",
    "@typescript-eslint/eslint-plugin": "^5.9.1",
    "@typescript-eslint/parser": "^5.9.1",
    "@vitejs/plugin-vue": "^2.2.0",
    "@vscode/test-electron": "^2.0.3",
    "autosize": "^5.0.1",
    "concurrently": "^7.0.0",
    "cron-parser": "^4.2.1",
    "crypto-js": "^4.1.1",
    "esbuild": "^0.14.22",
    "eslint": "^8.6.0",
    "esno": "^0.14.1",
    "fs-extra": "^10.0.0",
    "glob": "^7.2.0",
    "he": "^1.2.0",
    "husky": "^7.0.0",
    "i18next": "^21.6.11",
    "jose": "^4.5.0",
    "mocha": "^9.1.3",
    "monaco-editor": "0.32.1",
    "node-forge": "^1.2.1",
    "nodemon": "^2.0.15",
    "plop": "^3.0.5",
    "postcss": "^8.4.6",
    "qrcode": "^1.5.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-i18next": "^11.15.4",
    "regex-colorize": "^0.0.3",
    "rollup": "^2.67.3",
    "rollup-plugin-esbuild": "^4.8.2",
    "rollup-plugin-node-builtins": "^2.1.2",
    "rollup-plugin-node-globals": "^1.4.0",
    "rollup-plugin-postcss": "^4.0.2",
    "rollup-plugin-svelte": "^6.0.0",
    "semantic-release": "^19.0.2",
    "semantic-release-vsce": "^5.0.7",
    "svelte": "^3.0.0",
    "svelte-check": "^1.0.0",
    "svelte-i18n": "^3.3.13",
    "svelte-preprocess": "^4.0.0",
    "ts-loader": "^9.2.6",
    "typescript": "^4.5.4",
    "ua-parser-js": "^1.0.2",
    "uuid": "^8.3.2",
    "vite": "^2.8.4",
    "vue": "^3.2.29",
    "vue-i18n": "9",
    "webpack": "^5.66.0",
    "webpack-cli": "^4.9.1",
    "yaml": "^1.10.2"
  },
  "dependencies": {
    "@vscode/webview-ui-toolkit": "^0.9.1"
  }
}
