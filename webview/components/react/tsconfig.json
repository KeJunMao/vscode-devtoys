{
  "compilerOptions": {
    "types": [
      "react"
    ],
    "lib": [
      "esnext",
      "dom"
    ],
    "target": "ES2020",
    "strict": true,
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "allowSyntheticDefaultImports": true,
    "jsx": "react-jsx",
    "skipLibCheck": true,
  }
}
